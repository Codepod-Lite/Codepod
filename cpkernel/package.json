{
  "name": "cpkernel",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "postbuild": "copyfiles kernels/**/* dist/cjs/ && copyfiles kernels/**/* dist/mjs/",
    "watch": "watch 'tsc -p tsconfig.json && tsc -p tsconfig-cjs.json' ./src",
    "watch2": "watch 'ls' ./src",
    "build:watch": "tsc --watch -p tsconfig-cjs.json",
    "start": "nodemon --ignore '*.json' src/start-server.js",
    "migrate": "npx prisma migrate dev --name init",
    "resetdb": "npx prisma db push --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "3.5.0",
    "apollo-server": "^3.5.0",
    "apollo-server-express": "2.19",
    "bcryptjs": "^2.4.3",
    "dockerode": "^3.3.1",
    "graphql": "15",
    "jsonwebtoken": "^8.5.1",
    "prisma": "3.5.0",
    "stompjs": "^2.3.3",
    "uuid": "^8.3.2",
    "ws": "^8.2.3",
    "zeromq": "^6.0.0-beta.6"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.15",
    "typescript": "^4.4.4",
    "watch": "^1.0.2"
  }
}
